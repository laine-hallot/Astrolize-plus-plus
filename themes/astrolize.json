	"tokenColors": [
		{
			"name": "Source",
			"scope": [ "source" ],
			"settings": { "foreground": "#dddcd3" }
		},
		{
			"name": "Keyword (operator)",
			"scope": [ "keyword.operator" ],
			"settings": { "foreground": "#b4abbd" }
		},
		{
			"name": "Punctuation",
			"scope": [ "punctuation" ],
			"settings": { "foreground": "#adb8b8" }
		},
		{
			"name": "Punctuation (parenthesis)",
			"scope":
			[
				"punctuation.parenthesis",
				"meta.brace.round"
			],
			"settings": { "foreground": "#cdf5f0" }
		},
		{
			"name": "Punctuation (square bracket)",
			"scope":
			[
				"punctuation.squarebracket",
				"meta.brace.square"
			],
			"settings": { "foreground": "#c9d4e9" }
		},
		{
			"name": "Punctuation (curly brace)",
			"scope": [ "punctuation.curlybrace" ],
			"settings": { "foreground": "#bccee5" }
		},
		{
			"name": "Comment",
			"scope":
			[
				"comment",
				"comment punctuation",
				"comment punctuation.parenthesis",
				"comment punctuation.squarebracket",
			 	"comment punctuation.curlybrace",
				"comment keyword",
				"comment entity",
				"comment variable",
				"comment string"
			],
			"settings": { "foreground": "#a7afc2" }
		},


	

		{
			"name": "Literals (Constant)",
			"scope": [ "constant" ],
			"settings": { "foreground": "#bbeb98" } 
		},
		{
			"name": "Literals (null)",
			"scope": [ "constant.language.null" ],
			"settings": { "foreground": "#9cc480" }  
		},
		{
			"name": "Literals (Number)",
			"scope": [ "constant.numeric" ],
			"settings": { "foreground": "#c9d479" }
		},
		{
			"name": "Literals (String)",
			"scope":
			[
				"string.quoted.double",
				"string.quoted.double punctuation"
			],
			"settings": { "foreground": "#e4dd91" }
		},
		{
			"name": "Literals (Escape)",
			"scope": [ "constant.character.escape" ],
			"settings": { "foreground": "#cac376" } 
		},
		{
			"name": "Literals (Char)",
			"scope":
			[
				"string.quoted.single",
				"string.quoted.single punctuation"
			],
			"settings": { "foreground": "#7fc474" }
		},


		{
			"name": "Functions",
			"scope":
			[
				"entity.name.function",
				"entity.name.method",
				"keyword.other.get",
				"keyword.other.set",				
				"keyword.other.add",				
				"keyword.other.remove",
				"support.function",
				"keyword.operator.new"		
			],
			"settings": { "foreground": "#add6c5" }
		},


	

		{
			"name": "Functions (UnityEngine)",
			"scope": [ "support.function.UnityEngine" ],
			"settings": { "foreground": "#78ad92" } 
		},
		{
			"name": "Functions (UnityEditor)",
			"scope": [ "support.function.UnityEditor" ],
			"settings": { "foreground": "#78ad92" } 
		},
		{
			"name": "Functions (System)",
			"scope": [ "support.function.System" ],
			"settings": { "foreground": "#58806b" } 
		},


	

		{
			"name": "Types",
			"comment": "Including classes, delegates, enums, interfaces, structs.", 
			"scope":
			[
				"storage.type",
				"entity.name.type.type-parameter",
				"support.type",
				"support.class"
			],
			"settings": { "foreground": "#e7aac3" }
		},
		{
			"name": "Types (built-in)",
			"scope": [ "keyword.type" ],
			"settings": { "foreground": "#c491a7" } 
		},
		{
			"name": "Types (<>)",
			"scope": [ "punctuation.definition.typeparameters" ],
			"settings": { "foreground": "#a17788" } 
		},


	

		{
			"name": "Types (UnityEngine)",
			"scope": [ "support.type.unity.UnityEngine" ],
			"settings": { "foreground": "#c491a7" } 
		},
		{
			"name": "Types (UnityEditor)",
			"scope": [ "support.type.unity.UnityEditor" ],
			"settings": { "foreground": "#c491a7" } 
		},
		{
			"name": "Types (System)",
			"scope": [ "support.type.System" ],
			"settings": { "foreground": "#a17788" } 
		},


	

		{
			"name": "Modifiers",
			"scope": [ "storage.modifier" ],
			"settings": { "foreground": "#a17788" } 
		},


	

		{
			"name": "Types (declarations)",
			"comment": "Including classes, delegates, enums, interfaces, structs.",
			"scope": [ "entity.name.type" ],
			"settings": { "foreground": "#e4aaec" }
		},


	

		{
			"name": "Keywords",
			"scope":
			[
				"keyword.control",
				"keyword.operator.conditional",
				"keyword.query"
			],
			"settings": { "foreground": "#c6a0f2" }
		},
		{
			"name": "Keywords (other)",
			"scope": [ "keyword.other" ],
			"settings": { "foreground": "#987bba" } 
		},


	

		{
			"name": "Preprocessor",
			"scope":
			[
				"meta.preprocessor",
				"meta.preprocessor punctuation",
				"meta.preprocessor punctuation.parenthesis",
				"meta.preprocessor punctuation.squarebracket",
			 	"meta.preprocessor punctuation.curlybrace",
				"meta.preprocessor keyword"
			],
			"settings": { "foreground": "#9b83f2" } 
		},
		{
			"name": "Preprocessor (entities)",
			"scope":
			[
				"meta.preprocessor entity",
				"meta.preprocessor entity.name.method",				
				"meta.preprocessor entity.name.function",				
				"meta.preprocessor entity.name.variable",
				"meta.preprocessor variable",
				"meta.preprocessor string"
			],
			"settings": { "foreground": "#7260b3" } 
		},


	
		{
			"name": "Variables",
			"scope":
			[
				"variable.other.readwrite",
				"variable.other.object",
				"variable.other.alias",
				"entity.name.variable",
				"variable.other.property",
				"variable.other.constant",
				"variable.parameter",
				"variable.object.property",
				"variable.object.constant",
				"meta.object-literal.key",	
				"support.variable"	
			],
			"settings": { "foreground": "#83acf2" }
		},
		{
			"name": "Variables (local)",
			"scope":
			[
				"variable.other.object.property",
				"entity.name.variable.local",
				"keyword.other.base",
				"keyword.other.this",
				"variable.language.base",
				"variable.language.this"
			],
			"settings": { "foreground": "#6483b9" } 
		},



		{
			"name": "Variables (UnityEngine)",
			"scope": [ "support.variable.UnityEngine" ],
			"settings": { "foreground": "#6483b9" } 
		},
		{
			"name": "Variables (UnityEditor)",
			"scope": [ "support.variable.UnityEditor" ],
			"settings": { "foreground": "#6483b9" } 
		},
		{
			"name": "Variables (System)",
			"scope": [ "support.variable.System" ],
			"settings": { "foreground": "#506892" } 
		}
	]
}